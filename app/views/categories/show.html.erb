<div id="data-index" class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2 show-page">
		<% if notice != nil %>
			<p id="notice"><%= notice %></p>
		<% end %>

		<p>
		  <strong>Name:</strong>
		  <%= @category.cat_name %>
		</p>

		<%= link_to 'Edit', edit_category_path(@category), class: "button button-create" %>
		<%= link_to 'Back', categories_path, class: "button button-create" %>
	</div>
  </div>
</div>